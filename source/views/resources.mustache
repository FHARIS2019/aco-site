<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{bodyClass}}">
  {{> header }}
  <main class="main container-fluid">
    <div class="breadcrumbs" lang="en"><a href="{{ appUrl }}" title="{{ appName }}">{{ appName }}</a> &gt; {{ title }}</div>
    <h2>{{ title }}</h2>
    {{#content}}
    <div class="page-content">    
      {{#main}}
      <div class="{{class}}" lang="{{language_code}}" dir="{{language_dir}}">{{{html}}}</div>
      {{/main}}
    </div>
    {{/content}}
  </main>
  {{> footer }}
</body>
</html>