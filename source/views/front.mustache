<!doctype html>
<html>
  <head>
    {{> head }}
  </head>
  <body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{bodyClass}}">
    {{> header }}
    {{#content}}
    <div class="main container-fluid">
      {{#intro}}
        <div class="intro" lang="{{language_code}}" dir="{{language_dir}}">{{{html}}}</div>
      {{/intro}}
      {{> featured }}
    </div>
    {{/content}}    
    {{> footer }}
  </body>
</html>