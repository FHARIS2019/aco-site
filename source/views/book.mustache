<!doctype html>
<html lang="en">
<head>{{> head }}</head>
<body data-app="{{appUrl}}" data-appRoot="{{appRoot}}" class="{{bodyClass}}">
  {{> header }}
  <iframe
    role="main"
    title="Book Viewer: ACO"
    class="widget book"
    id="book"
    name="book"
    data-name="book"
    data-sourceUrl="{{sourceUrl}}"
    data-script='{ "js" : [ "crossframe.min.js", "book.min.js" ] }'
    allowfullscreen mozallowfullscreen webkitallowfullscreen>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
      <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TRVMLPF" height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
  </iframe>
  {{> loading }}
  {{{closure}}}
</body>
</html>
