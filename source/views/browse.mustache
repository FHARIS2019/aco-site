<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{ bodyClass }}">
  {{> header }}

  <main class="main container-fluid">

    <header>

      <div class="breadcrumbs arabic" lang="ar" dir="rtl"><a>المجموعات العربية على الانترنت</a> &gt; <a>تصفح المواضيع</a> &gt; النتائج</div>
      <div class="breadcrumbs" lang="en"><a href="{{ appUrl }}" title="{{ appName }}">{{ appName }}</a> &gt; {{ title }}</div>

      <h2>{{ title }}</h2>
      
      <!--
      {{> browseSelect }}
      -->
      
      <div class="resultsnum">Showing results <span class="start"></span> - <span class="docslength"></span> of <span class="numfound"></span></div>

    </header>

    <div 
      class="widget" 
      data-name="items" 
      data-lazyLoad="0" 
      data-rows="10" 
      data-paginator="0" 
      data-script='{ "js" : [ "browse.min.js" ], "hbs" : [ { "items" : "items.hbs" }, { "paginator" : "paginator.hbs" } ] }'
    ></div>
    
    <!--
    <div class="text-center">
      <ul class="pagination">
        <li class="disabled"><a href="#">&laquo;</a></li>
        <li class="active"><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">5</a></li>
        <li><a href="#">&raquo;</a></li>
      </ul>
    </div>
    -->
    
  </main>
  {{> footer }}

</body>

</html>